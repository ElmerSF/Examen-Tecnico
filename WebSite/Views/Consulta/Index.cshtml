@model WebSite.Models.Maestro
@{
    ViewBag.Title = "Consulta";

}
<div class="jumbotron">
    <h1>Consulta de Maestro </h1>
    <h2>Encabezado de la Factura</h2>
    <p>Al presionar Mostar Tabla Se muestran todas las facturas</p>
    <p>

        <a id="consulta" href="../Consulta/Tabla" class="btn btn-primary btn-lg">Mostrar Tabla Maestro &raquo;</a>
        <a id="consulta" href="../Modifica" class="btn btn-primary btn-lg">Modificacion Maestro &raquo;</a>
        <a id="consulta" href="../Detalle" class="btn btn-primary btn-lg">Consultar Detalle &raquo;</a>


    </p>
</div>

@using (Html.BeginForm("Tabla", "Consulta", FormMethod.Get))
{
    <div id="agregar">
        <p>Aqui aparecen las facturas</p>

    </div>
    <div class="row">
        <div class="col-lg-12">
            <table class="table table-bordered">
                <tr>
                    <th>Factura</th>
                    <th>Fecha</th>
                    <th>Cliente</th>
                    <th>Total</th>
                </tr>
            </table>

            @ViewBag.Message

        </div>

    </div>

}





